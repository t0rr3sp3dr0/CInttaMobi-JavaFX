<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<SplitPane xmlns:fx="http://javafx.com/fxml/1" dividerPositions="0.5" orientation="VERTICAL"
           xmlns="http://javafx.com/javafx/8"
           fx:controller="systems.singularity.cinttamobi.fachada.FornecedoresCadastroController">
    <items>
        <VBox>
            <children>
                <TableView fx:id="fornecedoresTableView" minHeight="120.0" VBox.vgrow="ALWAYS">
                    <columns>
                        <TableColumn fx:id="fornecedoresTableColumn" minWidth="256.0" prefWidth="256.0"
                                     text="Fornecedor"/>
                        <TableColumn fx:id="cFornecedoresTableColumn" minWidth="128.0" prefWidth="128.0"
                                     text="CPF/CNPJ"/>
                        <TableColumn fx:id="phoneNumberFornecedoresTableColumn" minWidth="128.0" prefWidth="128.0"
                                     text="Telefone"/>
                        <TableColumn fx:id="emailFornecedoresTableColumn" minWidth="192.0" prefWidth="192.0"
                                     text="e-mail"/>
                    </columns>
                </TableView>
                <HBox>
                    <children>
                        <ToolBar nodeOrientation="LEFT_TO_RIGHT" prefHeight="40.0" HBox.hgrow="ALWAYS">
                            <items>
                                <Button fx:id="addFornecedoresButton" mnemonicParsing="false">
                                    <graphic>
                                        <ImageView fitHeight="17.0" fitWidth="17.0">
                                            <image>
                                                <Image url="@/icons/ic_add.png"/>
                                            </image>
                                        </ImageView>
                                    </graphic>
                                </Button>
                                <Button fx:id="editFornecedoresButton" mnemonicParsing="false">
                                    <graphic>
                                        <ImageView fitHeight="17.0" fitWidth="17.0">
                                            <image>
                                                <Image url="@/icons/ic_edit.png"/>
                                            </image>
                                        </ImageView>
                                    </graphic>
                                </Button>
                                <Button fx:id="saveFornecedoresButton" mnemonicParsing="false">
                                    <graphic>
                                        <ImageView fitHeight="17.0" fitWidth="17.0">
                                            <image>
                                                <Image url="@/icons/ic_save.png"/>
                                            </image>
                                        </ImageView>
                                    </graphic>
                                </Button>
                            </items>
                        </ToolBar>
                        <ToolBar nodeOrientation="RIGHT_TO_LEFT" prefHeight="40.0" HBox.hgrow="ALWAYS">
                            <items>
                                <Button fx:id="deleteFornecedoresButton" mnemonicParsing="false">
                                    <graphic>
                                        <ImageView fitHeight="17.0" fitWidth="17.0">
                                            <image>
                                                <Image url="@/icons/ic_delete.png"/>
                                            </image>
                                        </ImageView>
                                    </graphic>
                                </Button>
                            </items>
                        </ToolBar>
                    </children>
                </HBox>
            </children>
        </VBox>
        <VBox>
            <children>
